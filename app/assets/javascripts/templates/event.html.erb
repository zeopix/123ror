<section class="row" ng-show="sports">
  <div class="row">
  <h1 class="h3 col-md-6"><span translate>outcomes</span> {{event.title}}Â </h1>
  <small><a href="#/sports/{{sport.id}}" translate>back</a></small>

  </div>
  <div class="row" ng-hide="event.outcomes.length > 0">
    <div class="text-center">
